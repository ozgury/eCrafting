{
    "name":"bootstrap",
    "type":"full",
    "description":"Bootstrap theme for calip.so",
    "version":"0.0.1",
    "author":"Mahir Yavuz",
    "layouts":{
        "default":{
            "layout":{
                "template":"default.html",
                "sections":{
                    "header":{
                        "template":"header.html",
                        "cache":true
                    },
                    "scripts":{
                        "template":"scripts.html",
                        "cache":true
                    },
                    "styles":{
                        "template":"styles.html",
                        "cache":true
                    },
                    "admin":{
                        "menu":"admin",
                        "template":"adminMenu.html"
                    },
                    "primary":{
                        "menu":"primary",
                        "template":"primaryMenu.html"
                    },
                    "secondary":{
                        "template":"secondaryMenu.html",
                        "cache":true
                    },
                    "messages":{
                        "template":"messages.html"
                    },
                    "body":{
                        "template":"body.html"
                    },
                    "sidePanel":{
                        "template":"sidePanel.html"
                    },
                    "footer":{
                        "template":"footer.html",
                        "cache":true
                    }
                }
            }
        },
        "home":{
            "layout":{
                "template":"default.html",
                "copyFrom":"default",
                "sections":{
                    "body":{
                        "template":"body.html",
                        "cache":true
                    }
                }
            }
        },
        "admin":{
            "layout":{
                "template":"admin.html",
                "copyFrom":"default",
                "sections":{
                    "scripts":{
                        "template":"scripts.html"
                    },
                    "body":{
                        "template":"body.html"
                    },
                    "footer":{
                        "template":"footer.html"
                    },
                    "sidePanel":{
                        "disable":true
                    }
                }
            }
        },
        "install":{
            "layout":{
                "template":"install.html",
                "copyFrom":"default",
                "sections":{
                    "body":{
                        "template":"body.html"
                    },
                    "footer":{
                        "template":"footer.html"
                    },
                    "sidePanel":{
                        "disable":true
                    }
                }
            }
        },
        "rss":{
            "layout":{
                "template":"rss.html",
                "sections":{
                    "body":{
                        "template":"body.html"
                    }
                }
            }
        }
    }
}